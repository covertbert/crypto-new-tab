{
  "description": "A new tab extension for converting cryptocurrencies",
  "version": "0.0.1",
  "name": "Crypto New Tab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "management"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}